---
import Layout from "@layouts/Default.astro";
import { Pill } from "@eliancodes/brutal-ui";
import { Button } from "@eliancodes/brutal-ui";
import { Card } from "@eliancodes/brutal-ui";
---

<Layout
  title="Projects"
  pageTitle="Bianca Tuliao | Projects"
  description="A summary of Bianca's projects"
>
  <main class="bg-pink p-6">
    <section id="projets" class="mt-8 grid md:grid-cols-2 gap-8 mx-auto max-w-screen-lg px-4">
      <h2 class="hidden">About Bianca</h2>
      <div class="flex flex-col md:col-span-2">
        <Card>
          <div class="flex flex-col justify-between items-start gap-4">
            <p class="mt-4 outfit text-2xl md:text-5xl lg:text-6xl">
              Under construction
            </p>
          </div>
        </Card>
      </div>
    </section>
  </main>
</Layout>
